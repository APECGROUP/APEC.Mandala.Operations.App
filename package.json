{
  "name": "tuanos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "switch-dev": "./build.sh dev && node scripts/enviroment.js dev",
    "switch-live": "./build.sh prod && node scripts/enviroment.js production",
    "android": "yarn switch-dev && react-native run-android",
    "android-live": "yarn switch-live && react-native run-android",
    "ios": "yarn switch-dev && react-native run-ios",
    "ios-live": "yarn switch-live && react-native run-ios",
    "start": "react-native start --reset-cache",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "android-build-APK": "rm -rf build && rm -rf android/build && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "android-build-AAB": "rm -rf build && rm -rf android/build && cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "open-folder-apk": "open ./android/app/build/outputs/apk",
    "open-folder-aab": "open ./android/app/build/outputs/bundle",
    "prepare": "husky install",
    "open-folder-ios": "open ios/TuanOs.xcworkspace",
    "openAndroidStudio": "open -a 'Android Studio' android",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "postinstall": "patch-package",
    "port": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
