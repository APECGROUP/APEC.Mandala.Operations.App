{
  "name": "mandalapurchasing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "switch-dev": "./build.sh dev && node scripts/enviroment.js dev",
    "switch-live": "./build.sh prod && node scripts/enviroment.js production",
    "android": "yarn switch-dev && react-native run-android && yarn port",
    "android-live": "yarn switch-live && react-native run-android",
    "ios": "yarn switch-dev && react-native run-ios",
    "ios-live": "yarn switch-live && react-native run-ios",
    "start": "react-native start --reset-cache",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ~/Library/Caches/CocoaPods && rm -rf ios/Pods && rm -f ios/Podfile.lock && watchman watch-del-all && rm -rf node_modules && rm -rf ios/build && yarn install && cd ios && pod install --verbose && cd ..",
    "android-build-APK": "rm -rf build && rm -rf android/build && cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && APK_PATH=$(find android/app/build/outputs/apk/release -name '*.apk' | head -n 1) && DATE=$(date +%d_%m_%Y) && mv \"$APK_PATH\" \"android/app/build/outputs/apk/release/Mandala-Purchasing-$DATE.apk\"",
    "android-build-AAB": "rm -rf build && rm -rf android/build && cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "open-folder-apk": "open ./android/app/build/outputs/apk",
    "open-folder-aab": "open ./android/app/build/outputs/bundle",
    "prepare": "husky install",
    "openXcode": "open ios/mandalapurchasing.xcworkspace",
    "openAndroidStudio": "open -a 'Android Studio' android",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "postinstall": "patch-package",
    "export-android": "mkdir -p android/output && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/output/index.android.bundle --assets-dest android/output",
    "export-ios": "mkdir -p ios/output && react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/output/main.jsbundle --assets-dest ios/output",
    "export-all": "yarn export-android && yarn export-ios",
    "port": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@notifee/react-native": "^9.1.8",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-firebase/app": "^22.2.0",
    "@react-native-firebase/messaging": "^22.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.0.19",
    "@react-navigation/native-stack": "^7.3.26",
    "@shopify/flash-list": "^2.0.0-alpha.11",
    "@tanstack/react-query": "^5.80.6",
    "@types/react-native-calendar-picker": "^8.0.0",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "husky": "^9.1.7",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "lodash": "^4.17.21",
    "lottie-react-native": "^7.2.2",
    "moment": "^2.30.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-native": "0.78.1",
    "react-native-auto-skeleton": "^0.1.25",
    "react-native-blob-util": "^0.22.2",
    "react-native-bootsplash": "^6.3.4",
    "react-native-calendar-picker": "^8.0.5",
    "react-native-date-picker": "5.0.10",
    "react-native-device-info": "^14.0.4",
    "react-native-error-boundary": "^2.0.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-crop-picker": "^0.51.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-keychain": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.5.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-ota-hot-update": "^2.3.0",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "^3.17.1",
    "react-native-rename": "^3.2.16",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.4",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.12.0",
    "react-native-toast-message": "^2.2.1",
    "react-native-version-check": "^3.5.0",
